import random

c = []
d = []

while True:
    try:
        listLength = int(input('Type in the length you want your lists to be: '))
        break
    except ValueError:
        print('enter an integer value please.')

print('Your two lists will contain randomly selected numbers.  Please choose the low and high end of the range in the next prompts.')


while True:
    while True:
        try:
            possListValLow = int(input('Type in the low end of the range of numbers here: ')) 
            break
        except ValueError:
            print('enter an integer value please.')

    while True:
        try:
            possListValHigh = int(input('Type in the high end of the range of numbers here: '))
            break
        except ValueError:
            print('enter an integer value please.')
    if possListValLow > possListValHigh:
        print('Your low number must be less than or equal to your high number value')
        continue
    else:
        break

def listGenerator(list):    
    num = random.randint(possListValLow, possListValHigh)
    return list.append(num)
    
while len(c) < listLength:
    listGenerator(c)
    c.sort()
print('List c is: ' + str(c))

while len(d) < listLength:
    listGenerator(d)
    d.sort()
print('List d is: ' + str(d))
